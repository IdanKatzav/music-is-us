@model IEnumerable<MusicIsUs.Models.Branches>
@{
    ViewBag.Title = "Contact";
}
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
@Scripts.Render("~/Scripts/MapHandler.js")
@Styles.Render("~/Content/Site.css")
<img id="logo" style="padding-bottom:100px; padding-top:50px;" class="center" src="../../pics/logo-white.jpeg">
<div class="videoPlayer">
    <iframe width="560" height="315" src="https://www.youtube.com/embed/rt4zLhT2opc" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>
<div class="videoPlayer">
    <iframe src="https://open.spotify.com/embed/playlist/37i9dQZEVXbJ6IpvItkve3" width="560" height="315" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
</div>
<h3 style="text-align : center">Contact Us</h3>



<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Street)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) 
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Street)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td><button class="displayOnMap btn" data-prop="@item.Street"/>View the Location on the map</td>
            </tr>
        }
    </tbody>
</table>
<h3 style="text-align : center">Our Offices</h3>
<iframe class="mapDisplay center"
         width="600"
        height="450"
        style="border:0"
        loading="lazy"
        allowfullscreen
        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBsG-gcX20RziyqnfWo6p-H44M4vbECMQ4&q=Tsahal+St+4, Ashdod"
></iframe>
<a class="twitter-timeline" href="https://twitter.com/MusicIsUs2?ref_src=twsrc%5Etfw">Tweets by MusicIsUs2</a>
<script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
<address>
    <strong>Support:</strong>   <a href="mailto:MusicIsUs789@gmail.com">MusicIsUs789@gmail.com</a><br />
</address>